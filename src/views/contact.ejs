          <br>
              <select name="selectMe" id="selectMe">
                  <option value="">Select a Contact Form</option>
                  <option value="content_1">General Inquiry</option>
                  <option value="content_2">Field Reservation</option>
                  <option value="content_3">Complaint</option>
                  <option value="content_4">Other</option>
              </select>
              <br>
              <br>
              <div id="content_1" class="group">
                <h4>General Inquiry</h4>
                <label>Enter in your general inquiry.</label>
                <textarea name="message" rows="10" cols="60"></textarea>
              </div>
              <br>
              
              <div id="content_2" class="group">

              Start Date : 
              <input type="date" name="Start Date" max="2100-12-31">
              Start Time : 
              <input type="time" name="Start Time"><br><br>
              End Date : 
              <input type="date" name="End Date" min="2018-10-28">
              End Time : 
              <input type="time" name="End Time"><br><br>
              <label for="field">Enter in any notes about your reservation</label>      
              <textarea name="Reservation Notes" rows="10" cols="60" id= "field" placeholder=""></textarea>
              </div><br>

              <div id="content_3" class="group">
                  <label >Enter your complaint</label>
                  <textarea name="Complaint" rows="10" cols="60" placeholder=""></textarea>
              </div>
              
              <div id="content_4" class="group">
                  <label >Enter your other message for the Fieldhouse Team.</label>
                  <textarea name="other message for the Fieldhouse Team" rows="10" cols="60" placeholder="">
                      </textarea>
              </div>

              <div class="form-group form-check">
                <input type="checkbox" class="form-check-input" id="exampleCheck1">
                <label class="form-check-label" for="exampleCheck1">Above Information is Legitimate</label>
              </div>
              <button type="submit" value = "send" class="btn btn-primary">Submit</button>
    </div>

